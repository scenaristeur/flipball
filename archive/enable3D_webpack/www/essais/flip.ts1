export { Flip }



class Flip {

  constructor(options) {
    console.log(options)
    Object.assign(this, options)
    let scene = this.scene
    let position = this.position
    this.tube_ext = this.scene.physics.add.cylinder(
      { height: 6,
         y: 2,
          x: position.x+2,
           radiusTop: 0.6,
            radiusBottom: 0.6,
             collisionFlags: 6, mass: 100 },
      // { custom: mat1 }
    )
    console.log(this)
  }

}
